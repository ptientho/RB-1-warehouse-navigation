<root BTCPP_format="4">
    <BehaviorTree ID="MainTree">
        <RetryUntilSuccessful num_attempts="3">
            <Sequence>
                <ShelfDetector name="detect_shelf" find_shelf="{shelf_found}"/>
                <CheckShelfFound name="flag_shelf_found" shelf_found="{shelf_found}"/>
            </Sequence>
        </RetryUntilSuccessful>
    </BehaviorTree>
</root>