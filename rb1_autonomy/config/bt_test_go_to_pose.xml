<root BTCPP_format="4">
    <BehaviorTree ID="MainTree">
        <RetryUntilSuccessful num_attempts="3">
            <Sequence>
                <ShelfDetector name="detect_shelf" front_offset="22.0" shelf_pose="{front_shelf}" find_shelf="{shelf_found}"/>
                <GoToPose2 name="go_to_shelf" pose="{front_shelf}"/>
            </Sequence>
        </RetryUntilSuccessful>
    </BehaviorTree>
</root>